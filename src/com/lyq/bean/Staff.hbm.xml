<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lyq.bean.Staff" table="staff">
        <id name="sid" column="ygId">
            <generator class="native"></generator>
        </id>
        <property name="sName" column="name"></property>
        <property name="sBase" column="base"></property>
        <property name="sAdd" column="overtime"></property>
        <property name="sPassword" column="password"></property>
     </class>
</hibernate-mapping>